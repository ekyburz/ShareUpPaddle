<p style="color: green"><%= notice %></p>

<h1>Bookings</h1>

<div id="bookings">
  <% @bookings.each do |booking| %>
    <%= render booking %>
    <p>
      <%= link_to "Show this booking", booking, class: "btn btn-primary" %>
    </p>
    <hr/>
  <% end %>
</div>

